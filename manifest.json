{
	"manifest_version": 3,
	"name": "Digital Stream Deck",
	"version": "1.1",
	"description": "Stream deck digital para Chrome",
	"icons": {
		"48": "images/icon.png"
	},
	"action": {
		"default_popup": "popup/popup.html",
		"default_icon": {
			"48": "images/icon.png"
		}
	},
	"permissions": ["storage", "activeTab", "webNavigation", "tabs"],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["scripts/shared.js"]
		}
	]
}
